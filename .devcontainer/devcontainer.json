{
  "name": "Album Viewer Dev Container",
  "dockerComposeFile": "../compose.yaml",
  "service": "devcontainer",
  "postAttachCommand": "npm run dev",
  "shutdownAction": "none",
  "workspaceFolder": "/app",
  "customizations": {
    "vscode": {
      "extensions": [
        "dbaeumer.vscode-eslint",
        "esbenp.prettier-vscode",
        "Vue.volar"
      ]
    }
  },
  "remoteUser": "node",
  "features": {
    "ghcr.io/devcontainers/features/git:1": {}
  }
}
