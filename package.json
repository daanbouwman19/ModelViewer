{
  "name": "mediaplayer-app",
  "version": "1.0.5",
  "description": "Media Player",
  "main": "out/main/index.js",
  "type": "module",
  "scripts": {
    "predev": "rimraf node_modules/better-sqlite3/build && npx electron-builder install-app-deps",
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "preview": "electron-vite preview",
    "pretest": "npm rebuild better-sqlite3",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "package": "npm run build && electron-builder",
    "package:win": "npm run build && electron-builder --win",
    "package:mac": "npm run build && electron-builder --mac",
    "package:linux": "npm run build && electron-builder --linux",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "verify": "npm run format && npm run lint && npm run typecheck && npm run test:coverage",
    "typecheck": "vue-tsc -b --noEmit",
    "dev:server": "tsx --watch src/server/main.ts",
    "dev:web": "vite",
    "predev:all": "npm rebuild better-sqlite3",
    "dev:all": "concurrently \"npm run dev:server\" \"wait-on tcp:3000 && npm run dev:web\"",
    "build:web": "vue-tsc -b --noEmit && vite build",
    "build:web:fast": "vite build",
    "build:server": "vite build --mode server",
    "build:all": "concurrently \"npm run build:web\" \"npm run build:server\"",
    "start:server:prod": "cross-env NODE_ENV=production node dist/server/index.js",
    "prestart:server:prod": "npm rebuild better-sqlite3",
    "test:e2e": "playwright test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/daanbouwman19/Mediaplayer"
  },
  "author": {
    "name": "Daan Bouwman",
    "email": "daan@example.com"
  },
  "license": "MIT",
  "build": {
    "publish": {
      "provider": "github",
      "releaseType": "release"
    },
    "appId": "com.daanbouwman.mediaplayer",
    "productName": "MediaPlayer",
    "electronLanguages": [
      "en-US"
    ],
    "compression": "normal",
    "npmRebuild": true,
    "buildDependenciesFromSource": false,
    "files": [
      "out/**/*",
      "!**/node_modules/*/{CHANGELOG.md,README.md,readme.md,license.txt,LICENSE.txt,test,tests,example,examples}"
    ],
    "directories": {
      "output": "release"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "category": "Graphics"
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true
    },
    "mac": {
      "target": [
        "dmg"
      ]
    },
    "asarUnpack": [
      "node_modules/better-sqlite3/**/*",
      "node_modules/ffmpeg-static/**/*"
    ]
  },
  "dependencies": {
    "better-sqlite3": "^12.6.2",
    "cors": "^2.8.6",
    "dotenv": "^17.2.4",
    "electron-log": "^5.4.3",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "ffmpeg-static": "^5.3.0",
    "google-auth-library": "^10.5.0",
    "googleapis": "^171.4.0",
    "helmet": "^8.1.0",
    "p-queue": "^9.1.0",
    "range-parser": "^1.2.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@swc/core": "^1.15.10",
    "@tailwindcss/vite": "^4.1.18",
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^25.2.1",
    "@types/range-parser": "^1.2.7",
    "@types/supertest": "^6.0.3",
    "@types/three": "^0.182.0",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "@vitejs/plugin-vue": "^6.0.4",
    "@vitest/coverage-istanbul": "^4.0.18",
    "@vue/test-utils": "^2.4.6",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^40.2.1",
    "electron-builder": "^26.7.0",
    "electron-vite": "^5.0.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-vue": "^10.6.2",
    "execa": "^9.6.1",
    "globals": "^17.3.0",
    "happy-dom": "^20.5.0",
    "hls.js": "^1.6.15",
    "prettier": "^3.8.1",
    "rimraf": "^6.0.1",
    "rollup-plugin-visualizer": "^6.0.5",
    "selfsigned": "^5.4.0",
    "supertest": "^7.2.2",
    "tailwindcss": "^4.1.17",
    "terser": "^5.44.1",
    "three": "^0.182.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.1",
    "vite": "^7.3.1",
    "vitest": "^4.0.6",
    "vue": "^3.5.27",
    "vue-tsc": "^3.2.3",
    "vue-virtual-scroller": "^2.0.0-beta.8",
    "wait-on": "^9.0.3"
  }
}
